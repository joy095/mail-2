		"@internationalized/date": "^3.8.2",
		"@lucide/svelte": "^0.515.0",
		"@sveltejs/adapter-auto": "^6.0.2",
		"@sveltejs/adapter-cloudflare": "^7.1.3",
		"@sveltejs/adapter-vercel": "^5.8.2",
		"@sveltejs/kit": "^2.27.3",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@tailwindcss/vite": "^4.1.11",
